<?xml version="1.0" encoding="UTF-8"?>
<launch>

    <!-- crazyflie -->
    <include file="$(find-pkg-share crazyflie)/launch/launch.py" >
        <arg name="backend" value="sim"/>
        <arg name="crazyflies_yaml_file" value="$(find-pkg-share crazyflo_planner)/config/crazyflies.yaml"/>
        <arg name="motion_capture_yaml_file" value="$(find-pkg-share crazyflo_planner)/config/motion_capture.yaml"/>
        <arg name="rviz_config_file" value="$(find-pkg-share crazyflo_planner)/config/config.rviz"/>
        <arg name="rviz" value="True"/>
        <arg name="gui" value="False"/>
    </include>

    <!-- crazyflie_planner -->
    <!-- <node pkg="crazyflo_planner" exec="mission" name="mission" output="screen"/> -->

    <!-- payload -->
    <node pkg="crazyflo_planner" exec="payload" name="payload" output="screen"/>
</launch>